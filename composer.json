{
    "name": "chiefgroup/laravel-esign",
    "description": "QF Laravel ESign API v2",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "ext-openssl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "doctrine/cache": "^1.10",
        "monolog/monolog": "^2.0",
        "pimple/pimple": "^3.0",
        "symfony/http-foundation": "^4.0 || ^5.0",
        "symfony/psr-http-message-bridge": "^1.0 || ^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "peng",
            "email": "2512422541@qq.com"
        }
    ],
 	"autoload": {
        "psr-4": {
            "QF\\LaravelEsign\\": "src/"
        }
    },
    "require-dev": {
    	"mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
    	"sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "QF\\LaravelEsign\\ServiceProvider"
            ]
        }
    }
}
